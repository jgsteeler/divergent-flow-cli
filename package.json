{
  "name": "divergent-flow-cli",
  "version": "0.1.0",
  "description": "Divergent Flow CLI - tools empowering neurodivergent minds to flow.",
  "main": "dist/index.js",
  "bin": {
    "divergent-flow": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "generate:api-client": "openapi --input http://localhost:8080/openapi.json --output src/api-client --client axios --useOptions --useUnionTypes",
    "test": "echo \"Error: no test specified\" && exit 1",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major"
  },
  "keywords": [
    "cli",
    "neurodivergent",
    "productivity",
    "divergent-flow"
  ],
  "author": "Johnny Gibson <jgibson@gibsonserviceco.com> (GSC Prod, a division of Gibson Service Company, LLC)",
  "contributors": [
    "GSC Prod <info@gibsonserviceco.com> (a division of Gibson Service Company, LLC)"
  ],
  "copyright": "Copyright (c) 2025 GSC Prod, a division of Gibson Service Company, LLC",
  "license": "MIT",
  "type": "commonjs",
  "devDependencies": {
    "@types/figlet": "^1.5.6",
    "@types/node": "^20.11.30",
    "axios": "^1.12.2",
    "openapi-typescript-codegen": "^0.29.0",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "boxen": "^7.1.1",
    "chalk": "^5.6.2",
    "commander": "^14.0.1",
    "figlet": "^1.9.3",
    "gradient-string": "^3.0.0",
    "inquirer": "^12.9.6",
    "tsyringe": "^4.10.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jgsteeler/divergent-flow-cli.git"
  },
  "bugs": {
    "url": "https://github.com/jgsteeler/divergent-flow-cli/issues"
  },
  "homepage": "https://github.com/jgsteeler/divergent-flow-cli#readme",
  "engines": {
    "node": ">=18.0.0"
  }
}
